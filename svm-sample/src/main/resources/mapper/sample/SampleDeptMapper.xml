<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="svm.sample.dao.SampleDeptMapper">
    <resultMap id="resultMap" type="svm.sample.entity.DeptDO">
        <id column="dept_id" property="id" />
        <result column="dept_name" property="name" />
        <result column="parent_id" property="parent" />
    </resultMap>
    <select id="queryAll" resultType="svm.sample.entity.DeptDO" resultMap="resultMap">
        select dept_id,dept_name,parent_id from sample_dept
    </select>
</mapper>